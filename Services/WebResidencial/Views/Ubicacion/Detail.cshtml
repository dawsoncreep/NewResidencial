@model IEnumerable<WebResidencial.Models.UbicacionUsuario>

@{
    ViewBag.Title = "Detail";
}

<h2>Usuarios en esta ubicación</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.Correo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.Celular)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario.Apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario.Correo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario.Celular)
        </td>
    </tr>
}

</table>
